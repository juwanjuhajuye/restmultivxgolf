<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    android:paddingBottom="0dp">

    <LinearLayout
        android:id="@+id/pushForReservationLinearLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="10dp"
        android:background="@drawable/roundlayout_pushpopup_weborders" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.3"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="6"
                android:gravity="center_vertical|left"
                android:paddingLeft="20dp"
                android:text=""
                android:textColor="#ffffff"
                android:textSize="20dp"
                android:textStyle="bold"/>
            <Button
                android:id="@+id/closeButton"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="8.5"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5">
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="25"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="10"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.1"
                        android:orientation="horizontal">
                    </LinearLayout>


                    <!-- 08302023 -->
                    <LinearLayout
                        android:id="@+id/pushForWebOrderOnlineType_ln"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="2"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/pushForWebOrderOnlineType"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:paddingRight="5dp"
                            android:textColor="#f8df00"
                            android:textSize="42dp"
                            android:textStyle="bold"
                            android:text="ONLINE TYPE"/>
                    </LinearLayout>
                    <!-- 08302023 -->


                    <LinearLayout
                        android:id="@+id/pushForWebOrderTableName_ln"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="2"
                        android:orientation="horizontal">
                        <TextView
                            android:visibility="gone"
                            android:id="@+id/pushForWebOrderTableIdx"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:paddingRight="5dp"
                            android:textColor="#f8df00"
                            android:textSize="40dp"
                            android:textStyle="bold"
                            android:text="Table Name"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:paddingRight="5dp"
                            android:textColor="#f8df00"
                            android:textSize="40dp"
                            android:textStyle="bold"
                            android:text="Table Info."/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.1"/>
                        <TextView
                            android:id="@+id/pushForWebOrderTableName"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2.2"
                            android:gravity="center_vertical|left"
                            android:paddingLeft="5dp"
                            android:textColor="#f8df00"
                            android:textStyle="bold"
                            android:textSize="40dp"
                            android:text="Table 01"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.1"
                        android:orientation="horizontal">
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:visibility="gone">
                        <TextView
                            android:id="@+id/pushForWebOrderTimeTo_title"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:paddingRight="5dp"
                            android:textColor="#3a8cde"
                            android:textSize="18dp"
                            android:textStyle="bold"
                            android:text="Time to Pickup"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.1"/>
                        <TextView
                            android:id="@+id/pushForWebOrderTimeTo"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2.2"
                            android:gravity="center_vertical|left"
                            android:paddingLeft="5dp"
                            android:textColor="#f8df00"
                            android:textStyle="bold"
                            android:textSize="18dp"
                            android:text="Pick up time"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.1"
                        android:orientation="horizontal">
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:paddingRight="5dp"
                            android:textColor="#3a8cde"
                            android:textSize="18dp"
                            android:textStyle="bold"
                            android:text="Receipt #"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.1"/>
                        <TextView
                            android:id="@+id/pushWebOrdersSalesCodeTextView"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2.2"
                            android:gravity="center_vertical|left"
                            android:paddingLeft="5dp"
                            android:textColor="#ffffff"
                            android:textSize="18dp"
                            android:text="Annie Jung"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.1"
                        android:orientation="horizontal">
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:paddingRight="5dp"
                            android:textColor="#3a8cde"
                            android:textSize="18dp"
                            android:textStyle="bold"
                            android:text="Order #"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.1"/>
                        <TextView
                            android:id="@+id/customerOrderNumTextView"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2.2"
                            android:gravity="center_vertical|left"
                            android:paddingLeft="5dp"
                            android:textColor="#ffffff"
                            android:textSize="18dp"
                            android:text="Annie Jung"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.1"
                        android:orientation="horizontal">
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:paddingRight="5dp"
                            android:textColor="#3a8cde"
                            android:textSize="18dp"
                            android:textStyle="bold"
                            android:text="Customer"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.1"/>
                        <TextView
                            android:id="@+id/pushWebOrdersCustomerTextView"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2.2"
                            android:gravity="center_vertical|left"
                            android:paddingLeft="5dp"
                            android:textColor="#ffffff"
                            android:textSize="18dp"
                            android:text="Juwan Kim"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.1"
                        android:orientation="horizontal">
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:paddingRight="5dp"
                            android:textColor="#3a8cde"
                            android:textSize="18dp"
                            android:textStyle="bold"
                            android:text="Phone"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.1"/>
                        <TextView
                            android:id="@+id/pushWebOrdersPhoneTextView"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2.2"
                            android:gravity="center_vertical|left"
                            android:paddingLeft="5dp"
                            android:textColor="#ffffff"
                            android:textSize="18dp"
                            android:text="718-555-9999"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.1"
                        android:orientation="horizontal">
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <!--
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:paddingRight="5dp"
                            android:textColor="#3a8cde"
                            android:textSize="18dp"
                            android:textStyle="bold"
                            android:text="Type"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.1"/>
                            -->
                        <TextView
                            android:id="@+id/pushWebOrdersDeliveryTakeawayTextView"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2.2"
                            android:gravity="center"
                            android:paddingLeft="5dp"
                            android:textColor="#ffffff"
                            android:textSize="18dp"
                            android:text="06-27-2016 15:00~16:15"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.1"
                        android:orientation="horizontal">
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:paddingRight="5dp"
                            android:textColor="#3a8cde"
                            android:textSize="18dp"
                            android:textStyle="bold"
                            android:text="Order Date"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.1"/>
                        <TextView
                            android:id="@+id/pushWebOrdersSalesDateTextView"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2.2"
                            android:gravity="center_vertical|left"
                            android:paddingLeft="5dp"
                            android:textColor="#ffffff"
                            android:textSize="18dp"
                            android:text="06-27-2016 15:00~16:15"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.1"
                        android:orientation="horizontal">
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.1"
                        android:orientation="horizontal">
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.5">
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5">
            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.5"
            android:background="#e9e9e9"
            android:orientation="horizontal"
            android:gravity="center">

            <Button
                android:id="@+id/pushForWebOrdersDetailViewButton"
                android:tag="pushForWebOrdersDetailViewButtonTag"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:background="@drawable/button_selector_realtimeweborders_ln1"
                android:textSize="20dp"
                android:textColor="#f70926"
                android:text="Detail View"/>

        </LinearLayout>




    </LinearLayout>

</LinearLayout>
