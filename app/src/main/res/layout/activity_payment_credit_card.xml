<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/rootLinearLayout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    android:paddingBottom="0dp">

    <LinearLayout
        android:id="@+id/paymentCreditCardLinearLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@drawable/roundlayout_line" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.8"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/paymentCreditCardTopTitleTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="left|center_vertical"
                    android:text="Payment Credit Card"
                    android:textSize="16dp"
                    android:paddingLeft="10dp"
                    android:textColor="#3e3d42" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:paddingLeft="10dp"
                android:gravity="right"
                android:orientation="horizontal">

                <ImageButton
                    android:layout_marginRight="20dp"
                    android:scaleType="centerInside"
                    android:src="@drawable/ab_imagebutton_card_status_in_3"
                    android:background="@color/zxing_transparent"
                    android:id="@+id/pcc_cardstatusbtn"
                    android:tag="pcc_cardstatusbtnTag"
                    android:layout_gravity="right"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.4"
                android:gravity="right|center_vertical"
                android:orientation="horizontal"
                android:paddingRight="10dp" >

                <Button
                    android:id="@+id/paymentCreditCardCloseBtn"
                    android:tag="paymentCreditCardCloseBtnTag"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:orientation="horizontal"
            android:background="#bbbbc8"></LinearLayout>



        <LinearLayout
            android:id="@+id/paymentCreditCardContentsLinearLayout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="8.5"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:orientation="horizontal">


            <!-- 좌측 컨텐츠 시작 -->

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.2"
                android:orientation="vertical"
                android:padding="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1.3"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/paymentCreditCardTitleTextView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="#0054d5"
                        android:textSize="20dp"
                        android:textStyle="bold"
                        android:text="CREDIT CARDS"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="8.5"
                        android:orientation="horizontal"
                        android:background="@drawable/roundlayout_line_noround">

                        <TextView
                            android:id="@+id/paymentCreditCardProcessingAmountTitleTextView"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center_vertical|left"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:textColor="#3f3f3f"
                            android:text="Processing Amount : "/>

                        <TextView
                            android:id="@+id/paymentCreditCardProcessingAmountTextView"
                            android:tag="paymentCreditCardProcessingAmountTextViewTag"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:textColor="#3f3f3f"
                            android:textSize="18dp"
                            android:textStyle="bold"
                            android:text="0.0"/>

                    </LinearLayout>

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.3"
                    android:orientation="horizontal"></LinearLayout>

                <LinearLayout
                    android:id="@+id/insertswipekeyinLinearLayout"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1.2"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5" />
                    <Button
                        android:id="@+id/paymentCreditCardInsertSwipeButton"
                        android:tag="paymentCreditCardInsertSwipeButtonTag"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="4"
                        android:gravity="center"
                        android:paddingLeft="8dp"
                        android:paddingRight="8dp"
                        android:background="@drawable/button_selector_paymentcreditcard_insertswipe"
                        android:textColor="#ffffff"
                        android:textSize="20dp"
                        android:text="Insert / Swipe"/>
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5" />
                    <Button
                        android:id="@+id/paymentCreditCardKeyInButton"
                        android:tag="paymentCreditCardKeyInButtonTag"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="4"
                        android:gravity="center"
                        android:paddingLeft="8dp"
                        android:paddingRight="8dp"
                        android:background="@drawable/button_selector_paymentcreditcard_keyin"
                        android:textColor="#ffffff"
                        android:textSize="20dp"
                        android:text="Key-In"/>
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.3"
                    android:orientation="horizontal"></LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="7.7"
                    android:orientation="vertical" >

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" >

                        <LinearLayout
                            android:id="@+id/linearLayout_frameMain"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:visibility="visible" >

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="1.4"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="0.5" />

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="8.5"
                                    android:orientation="vertical"
                                    android:background="@drawable/roundlayout_background_paymentgiftcard_cardnumber">

                                    <TextView
                                        android:id="@+id/paymentCreditCardCardNumberTitleTextView"
                                        android:layout_width="match_parent"
                                        android:layout_height="0dp"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:textColor="#3e3d42"
                                        android:text="CARD NUMBER"/>

                                    <TextView
                                        android:id="@+id/paymentCreditCardCardNumberTextView"
                                        android:layout_width="match_parent"
                                        android:layout_height="0dp"
                                        android:layout_weight="1.2"
                                        android:background="#ffffff"
                                        android:layout_marginLeft="10dp"
                                        android:layout_marginRight="10dp"
                                        android:layout_marginBottom="10dp"
                                        android:gravity="center"
                                        android:textColor="#6a6a6a"
                                        android:textSize="18dp"
                                        android:text="**** **** **** ****"/>

                                </LinearLayout>

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="0.5" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="0.3"
                                android:orientation="horizontal"></LinearLayout>


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="5"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="0.5" />

                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="8.5"
                                    android:orientation="horizontal">

                                    <TableLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_margin="0dp"
                                        android:stretchColumns="*">
                                        <TableRow
                                            android:layout_width="match_parent"
                                            android:layout_height="0dp"
                                            android:layout_weight="1" >
                                            <Button
                                                android:id="@+id/paymentCreditCardSuButton7"
                                                android:tag="paymentCreditCardSuButtonTag7"
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_weight="1"
                                                android:background="@drawable/button_selector_numberbutton_qty"
                                                android:layout_margin="3dp"
                                                android:text="7"
                                                android:textSize="20dp"
                                                android:textStyle="bold"
                                                android:textColor="#ffffff"/>
                                            <Button
                                                android:id="@+id/paymentCreditCardSuButton8"
                                                android:tag="paymentCreditCardSuButtonTag8"
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_weight="1"
                                                android:background="@drawable/button_selector_numberbutton_qty"
                                                android:layout_margin="3dp"
                                                android:text="8"
                                                android:textSize="20dp"
                                                android:textStyle="bold"
                                                android:textColor="#ffffff"/>
                                            <Button
                                                android:id="@+id/paymentCreditCardSuButton9"
                                                android:tag="paymentCreditCardSuButtonTag9"
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_weight="1"
                                                android:background="@drawable/button_selector_numberbutton_qty"
                                                android:layout_margin="3dp"
                                                android:text="9"
                                                android:textSize="20dp"
                                                android:textStyle="bold"
                                                android:textColor="#ffffff"/>
                                        </TableRow>
                                        <TableRow
                                            android:layout_width="match_parent"
                                            android:layout_height="0dp"
                                            android:layout_weight="1" >
                                            <Button
                                                android:id="@+id/paymentCreditCardSuButton4"
                                                android:tag="paymentCreditCardSuButtonTag4"
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_weight="1"
                                                android:background="@drawable/button_selector_numberbutton_qty"
                                                android:layout_margin="3dp"
                                                android:text="4"
                                                android:textSize="20dp"
                                                android:textStyle="bold"
                                                android:textColor="#ffffff"/>
                                            <Button
                                                android:id="@+id/paymentCreditCardSuButton5"
                                                android:tag="paymentCreditCardSuButtonTag5"
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_weight="1"
                                                android:background="@drawable/button_selector_numberbutton_qty"
                                                android:layout_margin="3dp"
                                                android:text="5"
                                                android:textSize="20dp"
                                                android:textStyle="bold"
                                                android:textColor="#ffffff"/>
                                            <Button
                                                android:id="@+id/paymentCreditCardSuButton6"
                                                android:tag="paymentCreditCardSuButtonTag6"
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_weight="1"
                                                android:background="@drawable/button_selector_numberbutton_qty"
                                                android:layout_margin="3dp"
                                                android:text="6"
                                                android:textSize="20dp"
                                                android:textStyle="bold"
                                                android:textColor="#ffffff"/>
                                        </TableRow>
                                        <TableRow
                                            android:layout_width="match_parent"
                                            android:layout_height="0dp"
                                            android:layout_weight="1" >
                                            <Button
                                                android:id="@+id/paymentCreditCardSuButton1"
                                                android:tag="paymentCreditCardSuButtonTag1"
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_weight="1"
                                                android:background="@drawable/button_selector_numberbutton_qty"
                                                android:layout_margin="3dp"
                                                android:text="1"
                                                android:textSize="20dp"
                                                android:textStyle="bold"
                                                android:textColor="#ffffff"/>
                                            <Button
                                                android:id="@+id/paymentCreditCardSuButton2"
                                                android:tag="paymentCreditCardSuButtonTag2"
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_weight="1"
                                                android:background="@drawable/button_selector_numberbutton_qty"
                                                android:layout_margin="3dp"
                                                android:text="2"
                                                android:textSize="20dp"
                                                android:textStyle="bold"
                                                android:textColor="#ffffff"/>
                                            <Button
                                                android:id="@+id/paymentCreditCardSuButton3"
                                                android:tag="paymentCreditCardSuButtonTag3"
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_weight="1"
                                                android:background="@drawable/button_selector_numberbutton_qty"
                                                android:layout_margin="3dp"
                                                android:text="3"
                                                android:textSize="20dp"
                                                android:textStyle="bold"
                                                android:textColor="#ffffff"/>
                                        </TableRow>
                                        <TableRow
                                            android:layout_width="match_parent"
                                            android:layout_height="0dp"
                                            android:layout_weight="1" >
                                            <Button
                                                android:id="@+id/paymentCreditCardSuButton0"
                                                android:tag="paymentCreditCardSuButtonTag0"
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_weight="1"
                                                android:background="@drawable/button_selector_numberbutton_qty"
                                                android:layout_margin="3dp"
                                                android:text="0"
                                                android:textSize="20dp"
                                                android:textStyle="bold"
                                                android:textColor="#ffffff"/>
                                            <Button
                                                android:id="@+id/paymentCreditCardSuButton00"
                                                android:tag="paymentCreditCardSuButtonTag00"
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_weight="1"
                                                android:background="@drawable/button_selector_numberbutton_qty"
                                                android:layout_margin="3dp"
                                                android:text="00"
                                                android:textSize="20dp"
                                                android:textStyle="bold"
                                                android:textColor="#ffffff"/>
                                            <Button
                                                android:id="@+id/paymentCreditCardSuButtonBack"
                                                android:tag="paymentCreditCardSuButtonTagBack"
                                                android:layout_width="0dp"
                                                android:layout_height="match_parent"
                                                android:layout_weight="1"
                                                android:background="@drawable/button_selector_numberbutton_qty"
                                                android:layout_margin="3dp"
                                                android:text="←"
                                                android:textSize="20dp"
                                                android:textStyle="bold"
                                                android:textColor="#ffffff"/>
                                        </TableRow>
                                    </TableLayout>

                                </LinearLayout>

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="0.5" />

                            </LinearLayout>


                            <LinearLayout
                                android:id="@+id/cardprocessinglistLn3"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="1.5"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="0.5" />

                                <Button
                                    android:id="@+id/paymentCreditCardCardProcessButton"
                                    android:tag="paymentCreditCardCardProcessButtonTag"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="8.5"
                                    android:background="@drawable/button_selector_paymentcreditcard_cardprocess"
                                    android:layout_margin="3dp"
                                    android:text="Card Process"
                                    android:textSize="20dp"
                                    android:textStyle="bold"
                                    android:textColor="#ffffff"/>

                                <TextView
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="0.5" />


                            </LinearLayout>



                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearLayout_frameInsertSwipe"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:visibility="invisible" >

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:textSize="22dp"
                                android:textStyle="bold"
                                android:text="Insert / Swipe Card"/>

                        </LinearLayout>


                        <LinearLayout
                            android:id="@+id/linearLayout_frameKeyIn"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:visibility="invisible" >

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:textSize="22dp"
                                android:textStyle="bold"
                                android:text="Key-In"/>

                        </LinearLayout>



                    </FrameLayout>
                </LinearLayout>




                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.3"
                    android:orientation="horizontal"></LinearLayout>

            </LinearLayout>

            <!-- 좌측 컨텐츠 종료 -->



            <!-- 우측 컨텐츠 시작 -->

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.5"
                android:orientation="vertical"
                android:paddingTop="20dp"
                android:paddingBottom="20dp"
                android:paddingRight="20dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:padding="1dp"
                    android:background="@drawable/roundlayout_line_noround">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1.5"
                        android:orientation="vertical"
                        android:background="#f0f0f0"
                        android:paddingLeft="15dp"
                        android:paddingRight="15dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/paymentCreditCardAmountTitleTextView"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.5"
                                android:gravity="center_vertical|left"
                                android:textColor="#4a5757"
                                android:textSize="14dp"
                                android:text="Amount" />

                            <TextView
                                android:id="@+id/paymentCreditCardAmountTextView"
                                android:tag="paymentCreditCardAmountTextViewTag"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="2"
                                android:gravity="center_vertical|right"
                                android:textColor="#4a5757"
                                android:textSize="16dp"
                                android:textStyle="bold"
                                android:text="0.0" />

                        </LinearLayout>

                        <!-- 07.18.2022 - add pay for cash, card -->
                        <LinearLayout
                            android:id="@+id/paymnetcreidtcartaddpayLn"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/paymnetcreidtcartaddpayTv"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center_vertical|right"
                                android:textColor="#48ca2d"
                                android:textSize="14dp"
                                android:text="" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/paymentCreditCardPaidTitleTextView"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.5"
                                android:gravity="center_vertical|left"
                                android:textColor="#4a5757"
                                android:textSize="14dp"
                                android:text="Paid" />

                            <TextView
                                android:id="@+id/paymentCreditCardPaidTextView"
                                android:tag="paymentCreditCardPaidTextViewTag"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="2"
                                android:gravity="center_vertical|right"
                                android:textColor="#4a5757"
                                android:textSize="16dp"
                                android:textStyle="bold"
                                android:text="0.0" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/paymentCreditCardBalanceTitleTextView"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1.5"
                                android:gravity="center_vertical|left"
                                android:textColor="#4a5757"
                                android:textSize="14dp"
                                android:text="Balance" />

                            <TextView
                                android:id="@+id/paymentCreditCardBalanceTextView"
                                android:tag="paymentCreditCardBalanceTextViewTag"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="2"
                                android:gravity="center_vertical|right"
                                android:textColor="#4a5757"
                                android:textSize="16dp"
                                android:textStyle="bold"
                                android:text="0.0" />

                        </LinearLayout>


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.1">
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/cardprocessinglistLn1"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="4"
                        android:padding="5dp"
                        android:orientation="vertical">

                        <ScrollView
                            android:id="@+id/paymentCreditCardProcessingCardListScrollView"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="10dp">
                            <LinearLayout
                                android:id="@+id/paymentCreditCardProcessingCardListLinearLayout"
                                android:tag="paymentCreditCardProcessingCardListTag"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">
                            </LinearLayout>
                        </ScrollView>

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.1">
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/cardprocessinglistLn2"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.8"
                        android:paddingBottom="10dp"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/paymentCreditCardVoidButton"
                            android:tag="paymentCreditCardVoidButtonTag"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="6.5"
                            android:background="@drawable/button_selector_paymentcreditcard_void"
                            android:layout_margin="3dp"
